<%- include('../partials/head.ejs') %>
<div class="d-flex justify-content-center">
<div class="container-md apply-list my-3 mx-2 content-align-center">
    <div class="row">
        <div class="col">
            <h2 class="text text-center">List of applicants</h2>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <ul class="text">
                <% locals.applications.forEach(application => { %>
                    <li><a href="/administration/review?id=<%= application._id %>" class="application px-2"><%= application.applier.username %> for <% if(application.application_perm_lvl === 2) {%>Redactor <%} else{ %>Admin <% }%></a></li>
                <% }); %> 
            </ul>
        </div>
    </div>
</div>
</div>
